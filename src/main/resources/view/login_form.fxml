<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="330.0" prefWidth="507.0" style="-fx-background-color: #0b243e;" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginFormController">
   <children>
      <TextField fx:id="txtUserName" layoutX="146.0" layoutY="116.0" prefHeight="26.0" prefWidth="214.0" promptText="User Name" style="-fx-border-color: #010201; -fx-border-radius: 10px;" />
      <PasswordField fx:id="txtPwd" layoutX="146.0" layoutY="175.0" prefHeight="26.0" prefWidth="214.0" promptText="Password" style="-fx-border-radius: 10px; -fx-border-color: #010201;" />
      <Button fx:id="btnLogin" layoutX="198.0" layoutY="257.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="46.0" prefWidth="110.0" style="-fx-background-color: #1b2f4a; -fx-border-color: #f4b41a;" text="LOGIN" textFill="#f4f1f1">
         <font>
            <Font name="Rockwell Bold" size="17.0" />
         </font></Button>
      <Label contentDisplay="CENTER" layoutX="148.0" layoutY="14.0" prefHeight="74.0" prefWidth="257.0" text="ADMIN LOGIN" textFill="WHITE">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <Button layoutX="234.0" layoutY="213.0" mnemonicParsing="false" onAction="#btnForgotPwdOnAction" prefHeight="26.0" prefWidth="121.0" text="Fotgot Password??" textFill="#1e1616" />
   </children>
</AnchorPane>
